<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <CFUser></CFUser>
    <CFPassword></CFPassword>
    <CFServerUri></CFServerUri>

    <CFAppMemory>512</CFAppMemory>
    <CFAppInstances>1</CFAppInstances>
    <CFStack>win2012r2</CFStack>
    <CFRoutes></CFRoutes>
    <CFServices></CFServices>
  
    <CFConfigurationFile></CFConfigurationFile>

    <CFAppName></CFAppName>
    <CFSpace></CFSpace>
    <CFLocalBuild>true</CFLocalBuild>

    <DeployTargetFile>C:\Path_to_targets_file_from_package</DeployTargetFile>
    <WebPublishMethod>CloudFoundry</WebPublishMethod>
    <CFMSBuildConfiguration>Debug</CFMSBuildConfiguration>
    <CFMSBuildPlatform>AnyCPU</CFMSBuildPlatform>

  </PropertyGroup>
</Project>